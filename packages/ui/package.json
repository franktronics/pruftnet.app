{
    "name": "@repo/ui",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "sideEffects": [
        "**/*.css"
    ],
    "exports": {
        "./atoms": "./src/atomic/atoms/index.ts",
        "./molecules": "./src/atomic/molecules/index.ts",
        "./organisms": "./src/atomic/organisms/index.ts",
        "./templates": "./src/atomic/templates/index.ts",
        "./hooks": "./src/hooks/index.ts",
        ".": "./src/index.ts",
        "./styles.css": "./src/styles/main.css"
    },
    "scripts": {
        "type-check": "tsc --noEmit",
        "build:css": "tailwindcss -i ./src/styles/main.css -o ./dist/index.css",
        "dev:css": "tailwindcss -i ./src/styles/main.css -o ./dist/index.css --watch"
    },
    "dependencies": {
        "@repo/utils": "workspace:*",
        "@tanstack/react-form": "^1.28.0",
        "@xyflow/react": "^12.10.0",
        "class-variance-authority": "^0.7.1",
        "d3-force": "^3.0.0",
        "html-to-image": "^1.11.13",
        "lucide-react": "^0.563.0",
        "radix-ui": "^1.4.3",
        "react-resizable-panels": "^4.6.2",
        "sonner": "^2.0.7",
        "tw-animate-css": "^1.4.0",
        "zod": "^4.3.6"
    },
    "peerDependencies": {
        "@tanstack/react-router": "^1.128.6",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@types/d3-force": "^3.0.10",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "tailwindcss": "^4.1.18",
        "typescript": "~5.9.3"
    }
}
